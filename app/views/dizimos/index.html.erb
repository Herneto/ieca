<p id="notice"><%= notice %></p>

<h1>Dizimos</h1>

<table>
  <thead>
    <tr>
      <th>Ano</th>
      <th>Jan</th>
      <th>Fev</th>
      <th>Mar</th>
      <th>Abr</th>
      <th>Mai</th>
      <th>Jun</th>
      <th>Jul</th>
      <th>Ago</th>
      <th>Set</th>
      <th>Out</th>
      <th>Nov</th>
      <th>Des</th>
      <th>Membro</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dizimos.each do |dizimo| %>
      <tr>
        <td><%= dizimo.ano %></td>
        <td><%= dizimo.jan %></td>
        <td><%= dizimo.fev %></td>
        <td><%= dizimo.mar %></td>
        <td><%= dizimo.abr %></td>
        <td><%= dizimo.mai %></td>
        <td><%= dizimo.jun %></td>
        <td><%= dizimo.jul %></td>
        <td><%= dizimo.ago %></td>
        <td><%= dizimo.set %></td>
        <td><%= dizimo.out %></td>
        <td><%= dizimo.nov %></td>
        <td><%= dizimo.des %></td>
        <td><%= dizimo.membro %></td>
        <td><%= link_to 'Show', dizimo %></td>
        <td><%= link_to 'Edit', edit_dizimo_path(dizimo) %></td>
        <td><%= link_to 'Destroy', dizimo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dizimo', new_dizimo_path %>
