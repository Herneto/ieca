<%= form_with(model: dizimo, remote: true, id: dizimo.id) do |form| %>
 
  <div class="modal-body">
        <div class="field hidden">
          <%= form.label :ano %>
          <%= form.text_field :ano, id: :dizimo_ano %>
        </div>

        <div class="field checkbox">
          <%= form.label :jan, "Janeiro" %>
          <%= form.check_box :jan, id: :dizimo_jan %>
        </div>

        <div class="field checkbox">
          <%= form.label :fev, "Fevereiro" %>
          <%= form.check_box :fev, id: :dizimo_fev %>
        </div>

        <div class="field checkbox">
          <%= form.label :mar, "MarÃ§o" %>
